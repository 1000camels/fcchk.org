/*Genaral*/
@media (max-width: 650px) {
	#front_top_slider .slider_box .slider_image img{width:1024px; height: 399px; }
	#front_top_slider .slider_box .slider_text .slider_title{ font-size: 20px !important;}
}
@media (max-width: 750px) {
	#top_search{clear: both;}
}
@media (max-width: 767px) {
	#front_map { height: 200px !important; }
	#front_map #huge_it_google_map1_container { margin-top: 20px !important; }
	#front_map #huge_it_google_map1 { height: 200px !important; }
	#front_map > a{top: 1px !important;}
}
@media (min-width: 800px) {
	.container{width: 750px;}
}
@media (min-width: 767px) and (max-width: 999px) {
	#front_map #huge_it_google_map1 { height: 112px !important; }
}
@media (min-width: 1000px){.container{width: 960px;}}
@media (min-width: 1200px){.container{width:1024px;}}
